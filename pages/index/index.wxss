.head {
    height: 50px;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}

.logo {
    width: 120px;
    height: 40px;
    padding: 10px;
}

.select {
    width: 40px;
    height: 40px;
    padding: 10px;
}

.image {
    padding-top: 10px;
    width: 750rpx;
    height: 400rpx;
}

.big {
    width: 100%;
    height: 100%;
}

.list1 {
    height: 90px;
    display: flex;
    flex-direction: row;
    justify-content: space-around;
}

.small1 {
    display: flex;
    flex-direction: column;
    justify-content: space-around;
}

.small {
    padding-top: 10px;
    width: 50px;
    height: 50px;
}

.text {
    font-size: 12px;
    padding-left: 5px;
}

.center {
    padding-top: 10px;
    height: 30px;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}

.text2 {
    padding-left: 10px;
    color: blue;
}

.text3 {
    padding-right: 10px;
    color: chartreuse;
}

.list-nav {
    display: flex;
    justify-content: space-between;
    padding: 0 10rpx;
}

.list-item {
    display: flex;
    margin-top: 20rpx;
    padding-left: 10px;
}

.list-item-left {
    width: 420rpx;
    height: 250rpx;
}

.list-item-left image {
    width: 100%;
    height: 100%;
}

.list-item-right {
    padding-right: 10px;
    font-size: 12px;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}

.list-item-right view:last-child {
    display: flex;
    justify-content: space-between;
}

.money {
    color: red;
}

.yellow {
    color: yellowgreen;
}

.xixi {
    color: skyblue;
}

.bigbox {
    display: flex;
    flex-wrap: wrap;
    flex-direction: row;
    justify-content: space-between;
}

.box2 {
    margin: 10px;
    height: 170px;
    width: 165px;
    /* background-color: springgreen; */
}

.image3 {
    width: 165px;
    height: 100px;
}

.en {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    padding-top: 20px;
}

.ha {
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}

.red1 {
    color: red;
}

.on {
    color: brown;
}

.tiancai {
    padding-top: 5px;
}